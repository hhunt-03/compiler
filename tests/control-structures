int true;
int false;

int result;
int i;

int main(){
    true = 1;
    false = 0;
    result = 0;

    if( true ){
        print(1);
    } 

    if( false ){
        print(1);
    } else {
        print(2);
    }

    if( 1 ){ 
        print(3);
    }

    if( !0 ){ 
        print(4); 
    }

    printlines(2);

    for(i = 0; i < 10; i = i + 1){
        result = result + 1;
    }   
    print(result);

    printlines(2);
    i = 0;
    while( i != 4 ){
        i = i + 1;
        print(i);
    }

}